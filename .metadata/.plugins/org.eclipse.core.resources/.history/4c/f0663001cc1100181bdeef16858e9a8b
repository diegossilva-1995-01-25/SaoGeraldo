package com.saogeraldo.beans;

import java.util.ArrayList;
import java.util.List;

import javax.annotation.PostConstruct;
import javax.faces.bean.ManagedBean;

@ManagedBean(name="teste")
public class TesteFotos {

	private List<String> images;
    
    @PostConstruct
    public void init() {
        images = new ArrayList<String>();
        
        images.add("/SiteSGeraldo/WebContent/images/Oracao.jpg");
        images.add("/SiteSGeraldo/WebContent/images/Ensino.jpg");
        images.add("/SiteSGeraldo/WebContent/images/Familia.jpg");
        
    }
 
    public List<String> getImages() {
        return images;
    }	
	
}
